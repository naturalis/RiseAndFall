      <species idref="species"/>
      <speciesTree idref="sptree"/>
    </nodeReHeight>
  </operators>
  
  <!-- mcmc settings -->
  <mcmc autoOptimize="true" chainLength="25000000" id="mcmc">
  
    <!-- posterior -->
    <posterior id="posterior">
    
      <!-- priors -->
      <prior id="prior">
        <speciesCoalescent idref="species.coalescent"/>
        <mixedDistributionLikelihood idref="species.popSize"/>
        <speciationLikelihood idref="speciation.likelihood"/>
        
          
          <!-- priors for clade0/clusters/cluster8.fa -->
          <logNormalPrior mean="1.0" meanInRealSpace="false" offset="0.0" stdev="1.25">
            <parameter idref="clade0/clusters/cluster8.fa.kappa"/>
          </logNormalPrior>
          <exponentialPrior mean="0.5" offset="0.0">
            <parameter idref="clade0/clusters/cluster8.fa.alpha"/>
          </exponentialPrior>
          <coalescentLikelihood idref="clade0/clusters/cluster8.fa.coalescent"/>
		  <booleanLikelihood>
			<monophylyStatistic idref="monophyly.ingroup.clade0/clusters/cluster8.fa"/>
		  </booleanLikelihood>
        
          
          <!-- priors for clade0/clusters/cluster12.fa -->
          <logNormalPrior mean="1.0" meanInRealSpace="false" offset="0.0" stdev="1.25">
            <parameter idref="clade0/clusters/cluster12.fa.kappa"/>
          </logNormalPrior>
          <exponentialPrior mean="0.5" offset="0.0">
            <parameter idref="clade0/clusters/cluster12.fa.alpha"/>
          </exponentialPrior>
          <coalescentLikelihood idref="clade0/clusters/cluster12.fa.coalescent"/>
		  <booleanLikelihood>
			<monophylyStatistic idref="monophyly.ingroup.clade0/clusters/cluster12.fa"/>
		  </booleanLikelihood>
        

        <!-- diversification priors -->
        <logNormalPrior mean="-7.0" stdev="2.0">
          <parameter idref="species.popMean"/>
        </logNormalPrior> 
        <logNormalPrior mean="4.6" stdev="2.0">
          <parameter idref="species.birthDeath.meanGrowthRate"/>
        </logNormalPrior>
        <betaPrior shape="1.0" shapeB="1.0">
          <parameter idref="species.birthDeath.relativeDeathRate"/>
        </betaPrior>

      </prior>
      
      <!-- likelihood -->
      <likelihood id="likelihood">
        
                
          <treeLikelihood idref="clade0/clusters/cluster8.fa.treeLikelihood"/>
        
                
          <treeLikelihood idref="clade0/clusters/cluster12.fa.treeLikelihood"/>
        
      </likelihood>
    </posterior>
    
    <!-- operators -->
    <operators idref="operators"/>
    
    <!-- where to log parameters -->
    <log fileName="./clade0/clade0.log" logEvery="1000">
      <posterior idref="posterior"/>
      <prior idref="prior"/>
      <likelihood idref="likelihood"/>
      <parameter idref="species.popMean"/>
      <parameter idref="speciesTree.splitPopSize"/>
      <parameter idref="species.birthDeath.meanGrowthRate"/>
      <parameter idref="species.birthDeath.relativeDeathRate"/>
      
        
        <parameter idref="clade0/clusters/cluster8.fa.treeModel.rootHeight"/>
        <parameter idref="clade0/clusters/cluster8.fa.kappa"/>
        <parameter idref="clade0/clusters/cluster8.fa.frequencies"/>
        <parameter idref="clade0/clusters/cluster8.fa.clock.rate"/>
        <treeLikelihood idref="clade0/clusters/cluster8.fa.treeLikelihood"/>
        <coalescentLikelihood idref="clade0/clusters/cluster8.fa.coalescent"/>
      
        
        <parameter idref="clade0/clusters/cluster12.fa.treeModel.rootHeight"/>
        <parameter idref="clade0/clusters/cluster12.fa.kappa"/>
        <parameter idref="clade0/clusters/cluster12.fa.frequencies"/>
        <parameter idref="clade0/clusters/cluster12.fa.clock.rate"/>
        <treeLikelihood idref="clade0/clusters/cluster12.fa.treeLikelihood"/>
        <coalescentLikelihood idref="clade0/clusters/cluster12.fa.coalescent"/>
      
    </log>
    
    <!-- where to log output species trees -->
    <logTree 
      fileName="./clade0/clade0.nex" 
      id="species.treeFileLog" 
      logEvery="1000" 
      nexusFormat="true" 
      sortTranslationTable="true">
      <speciesTree idref="sptree"/>
      <posterior idref="posterior"/>
    </logTree>
    
  </mcmc>
</beast>
